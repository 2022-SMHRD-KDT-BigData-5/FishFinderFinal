<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.smhrd.mapper.fishinfoMapper">
    
    <!-- 어종 정보 가져오기 -->
    <select id="fishList" resultType="com.smhrd.domain.fishinfo">
        select * from fish_info where fish_name=#{fish_name}
    </select>
      
      
  </mapper>
